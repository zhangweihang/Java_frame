   			<result>/jsp/demo2.jsp</result>
   		</action>
   	
   		<action name="demo3" class="com.ithc.demo3.Demo3ValueSatck" >
   			<result>/jsp/demo3.jsp</result>
   		</action>
   		
   		<action name="demo4" class="com.ithc.demo4.Demo4ValueStack">
   			
