<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--IOC 控制返回  -->
	<!-- 
		id 就是给类取一个标识符，一个spring的配置文件里面id不能有一样的
		class 类的全路径(包名+类名)
	 -->
   <bean id="userService" class="com.ihtc.spring.UserServiceImpl">
   </bean>
   <bean id="person" class="com.ihtc.spring.Person">
   		<property name="name" value="张三" />
   </bean>

	<bean id="car" class="com.ihtc.spring.Car">
		<property name="name" value="路虎"></property>
		<property name="price" value="123456"></property>
		<property name="list">
			<list>
				<value>哈哈</value>
				<value>嘻嘻</value>
				<value>嘎嘎</value>
			</list>
		</property>
		<property name="map">
			<map>
				<entry key="haha" value="xixi" />
				<entry key="beng" value="shakalaka" />
			</map>
		</property>
		<property name="strs">
			<list>
				<value>1</value>
				<value>2</value>
				<value>3</value>
			</list>
		</property>
		<property name="set">
			<set>
				<value>4</value>
				<value>5</value>
				<value>6</value>
			</set>
		</property>
		<property name="lp">
			<list>
				<ref bean="person" />
			</list>
		</property>
		<property name="sp">
			<set>
				<ref bean="person" />
				<ref bean="person" />
			</set>
		</property>
		<property name="mp">
			<map>
				<entry key="dasdas" value-ref="person" />
			</map>
		</property>
	</bean>
	
</beans>