		System.out.println("保存信息 ... ");
	}

	@Override
	public void update() {
		System.out.println("修改信息 ... ");
	}

	@Override
	public void delete() {
		System.out.println("删除信息 ... ");
	}

	@Override
	public void insert() {
		System.out.println("插入信息 ... ");
	}

}
